{
    "name": "eoinok/basic-template",
    "description": "a basic scaffold template designed to work with Infyom Laravel Generator.",
    "keywords": [
        "infyom",
        "laravel",
        "generator",
        "scaffold",
        "templates",
        "bootstrap",
        "bootstrap5"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eoin O'Kennedy",
            "email": "eokenned@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
                "laravel": {
                    "providers": [
                        "eoinok\\basic-template\\src\\BasicTemplateServiceProvider"
                    ],
                    "dont-discover": []
                }
            },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
    
    
}

